
public class AssignmentSix {

	public static void main(String[] args) 
	{
		int j=10;
		int x=(j*2)-4;
		for (int i = 1; i <= j; i++) 
			{
				System.out.print("$ ");
			}
		System.out.println();
		
		for (int i = 1; i < j-1; i++)
			{
				System.out.print("$ ");
				
				for (int l = 1; l <= x; l++)
					{
						System.out.print(" ");
					}
				System.out.println("$ ");
			}
		j--;
		
		for (int i = 1; i <= j+1; i++) 
		{
			System.out.print("$ ");
		}
		System.out.println();
	}

}
